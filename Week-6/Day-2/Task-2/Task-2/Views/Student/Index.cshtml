@model StudentAddressViewModel

<div class="container-fluid">
    <table class="table">
        <thead>
            <th>Roll No</th>
            <th>Name</th>
            <th>Class</th>
            <th>Address</th>
        </thead>
        <tbody>
            <td>@Model.StudentData.RollNo</td>
            <td>@Model.StudentData.Name</td>
            <td>@Model.StudentData.ClassSection</td>
            <td>@Model.Address.Street @Model.Address.City @Model.Address.State @Model.Address.Pincode</td>
        </tbody>
    </table>
</div>
